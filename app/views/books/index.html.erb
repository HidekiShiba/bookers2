<h2>Books</h2>
<table>
　<thead>
    <tr>
    　<th>Title</th>
    　<th>Body</th>
    </tr>
　</thead>
　<tbody>
    <% @books.each do |book| %>
    <tr>
      <%= attachment_image_tag user, :image %>
    　<td><%= book.title %></td>
    　<td><%= book.body %></td>
    </tr>
    <% end %>
　</tbody>
</table>