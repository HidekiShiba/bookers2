<!--User info、New book開始-->
<%= render 'users/list', user: @user %>
<!--User info、New book終了-->

<!--Users開始-->
<h2>Users</h2>
<table>
　<thead>
    <tr>
    　<th>image</th>
    　<th>name</th>
    </tr>
　</thead>
　<tbody>
    <% @users.each do |user| %>
    <tr>
    　<td><%= attachment_image_tag user, :profile_image, format: 'jpeg', fallback: "no_image.jpg", size: "60x60" %></td>
    　<td><%= user.name %></td>
    　<td><%= link_to "Show", user_path(user) %></td>
    </tr>
    <% end %>
　</tbody>
</table>
<!--Users終了-->