<h2>Users</h2>
<table>
　<thead>
    <tr>
    　<th>image</th>
    　<th>name</th>
    </tr>
　</thead>
　<tbody>
    <% @users.each do |user| %>
    <tr>
      <%= attachment_image_tag @user, :profile_image_id, format: 'jpeg', size: "60x60" %> <!--, fallback: "no_image.jpg"-->
    　<td><%= user.profile_image_id %></td>
    　<td><%= user.name %></td>
    </tr>
    <% end %>
　</tbody>
</table>